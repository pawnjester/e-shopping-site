<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/content_order_belongs_to"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:mContext="co.loystar.loystarbusiness.activities.OrderBelongsToActivity"
    tools:showIn="@layout/activity_order_belongs_to">

    <FrameLayout
        android:id="@+id/activity_order_belongs_to_container"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_above="@+id/activity_order_belongs_to_bottom_navigation_bar"
        android:animateLayoutChanges="true">

    </FrameLayout>

    <com.roughike.bottombar.BottomBar
        android:id="@+id/activity_order_belongs_to_bottom_navigation_bar"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:layout_alignParentBottom="true"
        app:bb_tabXmlResource="@xml/bottom_bar_tabs" />

</RelativeLayout>
